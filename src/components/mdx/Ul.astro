---
import { css } from 'styled-system/css';

type Props = astroHTML.JSX.DefinedIntrinsicElements['a'];

const props = Astro.props;
---

<ul
  {...props}
  class={css({
    fontSize: 'md',
    fontFamily: 'dosis',
    px: '6',
    listStyle: 'inside',
    listStylePosition: 'outside',
    w: '100%',
  })}
>
  <slot />
</ul>
